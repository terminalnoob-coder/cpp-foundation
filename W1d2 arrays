include <iostream>
using namespace std;
int main(){
  int count=0;
  int n;
  cin>>n;
  int arr[n];
    for(int i=0;i<n;i++)
    cin>>arr[i];
    for (int k=0;k<n;k++)
      { int m=0;
        for(int l=0;l<k;l++)
        {
          if (arr[l]==arr[k])
          m++;
        }
        if(m==0)
        count++;
      }
      cout<<count;
      return 0;
}
